{
  "name": "udacity-storefront",
  "version": "1.0.0",
  "description": "It is a simple server side application provide APIs to manage, interact with online store using tech stack nodejs, postgres, and express.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "postinstall": "npm-s install:**",
    "install:server": "cd ./server && npm start",
    "install:client": "cd ./client && npm start",
    "start": "run-s start:**",
    "start:server": "cd ./server && npm start",
    "start:client": "cd ./client && npm start",
    "build": "run-s build:**",
    "build:server": "cd ./server && npm run build",
    "build:client": "cd ./client && npm run build",
    "test": "run-s test:**",
    "test:server": "cd ./server && npm run test",
    "test:client": "cd ./client && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IMM9O/udacity-storefront.git"
  },
