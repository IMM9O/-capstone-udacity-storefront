{
  "name": "udacity-storefront",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "cd ./client && npm i && npm run db:up",
    "lint": "eslint . --ext .ts",
    "watch": "tsc-watch --esModuleInterop src/index.ts --outDir ./dist --onSuccess 'node ./dist/index.js'",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "jasmine": "jasmine",
    "test": "cross-env NODE_ENV=test db-migrate reset --env test && db-migrate up --env test && jasmine-ts",
    "test:windows": "set NODE_ENV=test& db-migrate reset --env test && db-migrate up --env test && jasmine-ts",
    "build": "npx tsc",
    "start": "run-p start:**",
    "start:server": "nodemon src/index.ts",
    "start:client": "cd ./client && npm start",
    "db:up": "db-migrate up",
    "db:down": "db-migrate reset"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/node": "^16.11.11",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.4.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.10.0",
    "jasmine-spec-reporter": "^7.0.0",
    "jasmine-ts": "^0.4.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "supertest": "^6.1.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1"
  }
}
